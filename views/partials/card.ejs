<% data.forEach(POST => { %>
<a href="/post/<%= POST._id %>">
  <div class="card_wrapper">
    <div class="card_img">
      <img
        src="<%= POST.blog_img %>"
        alt="card_img"
        width="185px"
        height="185px"
      />
    </div>
    <div class="blog_title"><h3><%= POST.title %></h3></div>
  </div>
</a>

<% }) %>
