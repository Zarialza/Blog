<div class="admin_title">
  <h2>Posts</h2>
  <a href="/add-post"> Add New Post</a>
</div>

<ul class="admin-post">
  <% data.forEach(post => { %>
  <li class="admin-list">
    <a href="/post/<%= post._id %>"> <%= post.title %> &nearr; </a>
    <div class="admin-post-controls">
      <a href="/edit-post/<%= post._id  %>" class="adminbtn">Edit</a>

      <form action="/delete-post/<%= post._id %>?_method=DELETE" method="POST">
        <input type="submit" value="Delete" class="adminbtn" />
      </form>
    </div>
  </li>

  <% }) %>
</ul>
